<p id="notice"><%= notice %></p>

<h1>Sign In Meal Plan</h1>

<table>
  <thead>
    <tr>
      
      <th>Plan </th>
      <th>Meal recipes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @signs.each do |sign| %>
      <tr>
        <td><%= sign.email %></td>
        <td><%= sign.password %></td>
        <td><%= link_to 'Show', sign %></td>
        <td><%= link_to 'Edit', edit_sign_path(sign) %></td>
        <td><%= link_to 'Destroy', sign, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to ' Sign In ', new_sign_path %>
